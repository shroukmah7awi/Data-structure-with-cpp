
using namespace std;
#include <iostream>

struct node
{
	int data;
	node* next;
};
node* head = NULL;

void insertnode(int val)
{
	node* new_node, * last;
	new_node = new node;
	new_node->data = val;


	if (head == NULL)
	{
		head = new_node;
		new_node->next = NULL;

	}
	else
	{
		last = head;

		while (last->next != NULL)
		{
			last = last->next;
		}
		last->next = new_node;
		new_node->next = NULL;
	}
}


void print()
{
	node* current_node;
	if (head == NULL)
	{
		cout << "linked list is empty";
	}
	else
	{
		current_node = head;
		while (current_node != NULL)
		{
			cout<<current_node->data<<"\t";
			current_node = current_node->next;
		}
	}
}


int main()
{
	
	insertnode(1);
	insertnode(2);
	insertnode(3);
	insertnode(4);
	print();
}
